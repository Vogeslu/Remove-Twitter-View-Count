{
    "name": "Remove Twitter View Count",
    "version": "1.0.1",
    "description": "Browser extension to remove the view count from tweets",
    "permissions": [],
    "content_scripts":[
      {
       "matches":["https://twitter.com/*"],
       "js":["main.js"]
      }
    ],
    "page_action": {
        "default_icon": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
        }
    },
    "icons": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "manifest_version": 2
}